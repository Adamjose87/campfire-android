<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <!--suppress AndroidUnknownAttribute -->
    <data class=".PrivacyConsentBinding" />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="@dimen/content_padding"
        android:paddingEnd="@dimen/new_playlist_input_padding_end"
        android:paddingStart="@dimen/new_playlist_input_padding_start"
        android:paddingTop="@dimen/large_content_padding">

        <CheckBox
            android:id="@+id/checkbox"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingEnd="0dp"
            android:paddingStart="@dimen/content_padding"
            android:text="@string/home_privacy_policy_checkbox" />

    </FrameLayout>

</layout>